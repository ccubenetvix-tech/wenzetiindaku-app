# Wenze Tii Ndaku App Environment Variables
# Copy this file to .env.local and fill in your values
# NEVER commit .env.local to version control

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://api.wenzetiindaku.com/v1
EXPO_PUBLIC_API_TIMEOUT=30000

# App Configuration
EXPO_PUBLIC_APP_ENV=production

# Feature Flags (set to 'true' or 'false')
EXPO_PUBLIC_ENABLE_MOCK_DATA=false
EXPO_PUBLIC_ENABLE_DEBUG_LOGS=false
# Google OAuth Configuration
# Get these from Google Cloud Console -> APIs & Services -> Credentials
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your-web-client-id.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your-ios-client-id.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your-android-client-id.apps.googleusercontent.com